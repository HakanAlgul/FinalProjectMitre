@model List<FinalProjectMitre.Models.Hardware_Additions>
@{ ViewBag.Title = "Hardware"; }
<div class=" p-3 text-center">
    <h2>Credentials from Password Stores Histories</h2>
</div>
<div class="container">
    <table class="table table-dark">
        <thead>
            <tr>
                <th scope="col">ID</th>
                <th scope="col">Result</th>
                <th scope="col">Device</th>
                <th scope="col">Date</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var item in Model)
            {
                <tr>

                    <td> @item.customer_id</td>
                    <td> @item.result</td>
                    <td> @item.checked_device</td>
                    <td> @item.date</td>
                </tr>
            }
        </tbody>
    </table>
</div>